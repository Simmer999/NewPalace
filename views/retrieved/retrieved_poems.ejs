<section>
    <form   class="title" method="PATCH">
        <h2>Retrieved Poems</h2>
        <br>
        <br>
        <ul>
        <% for( var i = 0; i < poems.length; i++) { %>
            <li class="poem">
                <span><%= poems[i].title %> </span>
                <br>
                <span><%= poems[i].author %></span>
            </li>
            <br>
            <% } %>
            <br>
        </ul>
    </form>
</section>
