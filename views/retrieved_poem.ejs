<%- include('./partials/head'); %>
<section>
    <%- include('./partials/nav'); %>
    <form   class="title" method="PATCH">
        <h2>Retrieved Poems</h2>
        <br>
        <br>
        <ul>
        <% for( var i = 0; i < poems_texts.length; i++) { %>
            <li class="poem">
                <span><%= poems_texts[i].title %> </span>
                <br>
                <span><%= poems_texts[i].author %></span>
            </li>
            <br>
            <% } %>
            <br>
        </ul>
    </form>
</section>
<%- include('./partials/footer'); %>